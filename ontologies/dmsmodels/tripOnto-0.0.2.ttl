###  http://www.semanticweb.org/dmonto/autology/trip#hasRoadSegment
:hasRoadSegment rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:range :RoadSegment ;
                rdfs:label "part of the route"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#hasRoute
:hasRoute rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:InverseFunctionalProperty ;
          rdfs:domain :Trip ;
          rdfs:range :Route ;
          rdfs:label "route"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#hasRouteLength
:hasRouteLength rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Route ;
                rdfs:range :Distance .


###  http://www.semanticweb.org/dmonto/autology/trip#hasSpeed
:hasSpeed rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:range :Speed .


###  http://www.semanticweb.org/dmonto/autology/trip#hasSpeedUnit
:hasSpeedUnit rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasUnit ;
              rdf:type owl:FunctionalProperty ;
              rdfs:range :SpeedUnit .


###  http://www.semanticweb.org/dmonto/autology/trip#hasUnit
:hasUnit rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.semanticweb.org/dmonto/autology/trip#onRoadSegment
:onRoadSegment rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :RoadMatchedMotionStep ;
               rdfs:range :RoadSegment ;
               rdfs:label "located in road part"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#startsAtNode
:startsAtNode rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :RoadSegment ;
              rdfs:range :Node .


