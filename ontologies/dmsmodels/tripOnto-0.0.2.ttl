###  http://www.semanticweb.org/dmonto/autology/trip#hasNumericValue
:hasNumericValue rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:range xsd:float .


###  http://www.semanticweb.org/dmonto/autology/trip#hasRoadName
:hasRoadName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :RoadSegment ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasSegmentID
:hasSegmentID rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :RoadSegment ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasShape
:hasShape rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:range geosparql:wktLiteral .


###  http://www.semanticweb.org/dmonto/autology/trip#hasStartTimestamp
:hasStartTimestamp rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty .


###  http://www.semanticweb.org/dmonto/autology/trip#hasTZID
:hasTZID rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasTZOffset
:hasTZOffset rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:integer .


###  http://www.semanticweb.org/dmonto/autology/trip#hasTimestamp
:hasTimestamp rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain time:Instant ;
              rdfs:range xsd:dateTime ;
              rdfs:label "date and time"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#hasTripID
:hasTripID rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Trip ;
           rdfs:range xsd:string ;
           rdfs:label "trip ID"@en .


