#    Data properties
#################################################################

###  http://www.semanticweb.org/dmonto/autology/trip#hasAbsLocations
:hasAbsLocations rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Trace ;
                 rdfs:range geosparql:wktLiteral ;
                 rdfs:comment "This relation connects Route with GeoSPARQL WKT literal which in turn must has value LINESTRING in http://www.opengis.net/def/crs/OGC/1.3/CRS84 coordinate system by default."@en ;
                 rdfs:label "geo points"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#hasAvgSpeed
:hasAvgSpeed rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:float .


###  http://www.semanticweb.org/dmonto/autology/trip#hasDriverID
:hasDriverID rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Driver ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasFirstAbsLocation
:hasFirstAbsLocation rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:range geosparql:wktLiteral .


###  http://www.semanticweb.org/dmonto/autology/trip#hasFirstLocationName
:hasFirstLocationName rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasFirstName
:hasFirstName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasLastAbsLocation
:hasLastAbsLocation rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:range geosparql:wktLiteral .


###  http://www.semanticweb.org/dmonto/autology/trip#hasLastLocationName
:hasLastLocationName rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasLastName
:hasLastName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#hasMaxSpeed
:hasMaxSpeed rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:float .


###  http://www.semanticweb.org/dmonto/autology/trip#hasMinSpeed
:hasMinSpeed rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
