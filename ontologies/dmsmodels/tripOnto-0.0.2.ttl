@prefix : <http://www.semanticweb.org/dmonto/autology/trip#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns: <http://creativecommons.org/ns#> .
@prefix ns2: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix otn: <http://www.pms.ifi.uni-muenchen.de/OTN#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wot: <http://xmlns.com/wot/0.1/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@base <http://www.semanticweb.org/dmonto/autology/trip> .

<http://www.semanticweb.org/dmonto/autology/trip> rdf:type owl:Ontology ;
                                                  owl:imports <http://www.opengis.net/ont/geosparql> ,
                                                              <http://www.w3.org/2006/time#2016> ;
                                                  dc:date "2019-09-10"^^xsd:date ;
                                                  dc:language "EN"^^xsd:string ;
                                                  rdfs:comment """This ontology is for roads and traffic on roads. It considers only vehicles, intersections and speed limits.

Questions to answer
1) What is Trip1 route on 2019-09-10?
2) What is characteristics of Trip1?
3) Which segments is covered by Trip1?
4) On which segments of Trip1 vehicle was over speeding?
5) On which intersections vehicle from Trip1 did not stop on Stop sign?
6) Which intersections vehicle from Trip1 passed?
7) Did Trip1 pass only one intersection?"""^^xsd:string ;
                                                  rdfs:label "Traffic on roads Ontology"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/language
dc:language rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/dmonto/autology/trip#byVehicle
:byVehicle rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Trip ;
           rdfs:range :Vehicle ;
           rdfs:label "trip by vehicle"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#drivenBy
:drivenBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Trip ;
          rdfs:range :Driver ;
          rdfs:label "vehicle driver"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#endsAtNode
:endsAtNode rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :RoadSegment ;
            rdfs:range :Node .


###  http://www.semanticweb.org/dmonto/autology/trip#hasAverageSpeed
:hasAverageSpeed rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:range :Speed .


###  http://www.semanticweb.org/dmonto/autology/trip#hasDistanceUnit
:hasDistanceUnit rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasUnit ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:range :DistanceUnit .


###  http://www.semanticweb.org/dmonto/autology/trip#hasDriverClass
:hasDriverClass rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Driver ;
                rdfs:range :DriverClass .


###  http://www.semanticweb.org/dmonto/autology/trip#hasFurniture
:hasFurniture rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :RoadSegment ;
              rdfs:range :RoadFurniture .


###  http://www.semanticweb.org/dmonto/autology/trip#hasL1Label
:hasL1Label rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :MotionStep ;
            rdfs:range :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#hasLinkLength
:hasLinkLength rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :RoadSegment ;
               rdfs:range :Distance .


###  http://www.semanticweb.org/dmonto/autology/trip#hasMotionStep
:hasMotionStep rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Trace ;
               rdfs:range :MotionStep ;
               rdfs:label "motion point"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#hasRestriction
:hasRestriction rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :RoadSegment ;
