                  rdfs:range xsd:float ;
                  rdfs:label "over speeding"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#sharpSpeedDropByValue
:sharpSpeedDropByValue rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain :DriverHardBrakingEvent ;
                       rdfs:range xsd:float ;
                       rdfs:label "sharp drop of speed"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/dmonto/autology/trip#Distance
:Distance rdf:type owl:Class ;
          rdfs:subClassOf :Quantity ,
                          [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasDistanceUnit ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :DistanceUnit
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasNumericValue ;
                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] .


###  http://www.semanticweb.org/dmonto/autology/trip#DistanceUnit
:DistanceUnit rdf:type owl:Class ;
              rdfs:subClassOf :MeasureUnit .


###  http://www.semanticweb.org/dmonto/autology/trip#Driver
:Driver rdf:type owl:Class ;
        rdfs:comment """Constraints
:hasDriverID exactly 1 rdfs:Literal"""^^xsd:string ;
        rdfs:label "driver"@en .


###  http://www.semanticweb.org/dmonto/autology/trip#DriverClass
:DriverClass rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#DriverHardBrakingEvent
:DriverHardBrakingEvent rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :MotionStep
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :sharpSpeedDropByValue ;
                                                                     owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf :Event ;
                        rdfs:comment ":MotionStep and (:sharpSpeedDropByValue exactly 1 rdfs:Literal)"^^xsd:string ;
                        rdfs:label "during trip driver was braking hard in some places on the road"^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#DriverOverspeedingEvent
:DriverOverspeedingEvent rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :MotionStep
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :overspeedByValue ;
                                                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :Event ;
                         rdfs:comment ":MotionStep and (:overspeedByValue exactly 1 rdfs:Literal)"^^xsd:string ;
                         rdfs:label "during trip driver was over speeding in some places on the road"^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#DriverStoppedEvent
:DriverStoppedEvent rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :MotionStep
                                                               [ rdf:type owl:Class ;
                                                                 owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :hasSpeed ;
                                                                                 owl:hasValue :zero_speed
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :hasMinSpeed ;
                                                                                 owl:hasValue 0.0
                                                                               ]
                                                                             )
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Event ;
                    rdfs:comment """:MotionStep
 and ((:hasSpeed value :zero_speed) or (:hasMinSpeed value 0.0))"""^^xsd:string ;
                    rdfs:label "during trip driver has stopped in some places on the road"^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#Event
:Event rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#Intersection
:Intersection rdf:type owl:Class ;
              rdfs:subClassOf :Junction ;
              rdfs:comment """TODO
define to use for intersections"""^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#Junction
:Junction rdf:type owl:Class ;
          rdfs:subClassOf :Node .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Acceleration
:L1Acceleration rdf:type owl:Class ;
                rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1AccelerationLat
:L1AccelerationLat rdf:type owl:Class ;
                   rdfs:subClassOf :L1Acceleration ;
                   owl:disjointWith :L1AccelerationVert .


###  http://www.semanticweb.org/dmonto/autology/trip#L1AccelerationLon
:L1AccelerationLon rdf:type owl:Class ;
                   rdfs:subClassOf :L1Acceleration .


###  http://www.semanticweb.org/dmonto/autology/trip#L1AccelerationVert
:L1AccelerationVert rdf:type owl:Class ;
                    rdfs:subClassOf :L1Acceleration .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Brake
:L1Brake rdf:type owl:Class ;
         rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1DBrake
:L1DBrake rdf:type owl:Class ;
          rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1DSpeed
:L1DSpeed rdf:type owl:Class ;
          rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1DSteering
:L1DSteering rdf:type owl:Class ;
             rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1DThrottle
:L1DThrottle rdf:type owl:Class ;
             rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Label
:L1Label rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Speed
:L1Speed rdf:type owl:Class ;
         rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Steering
:L1Steering rdf:type owl:Class ;
            rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#L1Throttle
:L1Throttle rdf:type owl:Class ;
            rdfs:subClassOf :L1Label .


###  http://www.semanticweb.org/dmonto/autology/trip#MaxSpeedLimitRestriction
:MaxSpeedLimitRestriction rdf:type owl:Class ;
                          rdfs:subClassOf [ owl:intersectionOf ( :Restriction
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasSpeedUnit ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :SpeedUnit
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasNumericValue ;
                                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] .


###  http://www.semanticweb.org/dmonto/autology/trip#MeasureUnit
:MeasureUnit rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#MotionPoint
:MotionPoint rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :MotionStep
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasSpeed ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Speed
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasTimestamp ;
                                                          owl:cardinality "1"^^xsd:nonNegativeInteger
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] .


###  http://www.semanticweb.org/dmonto/autology/trip#MotionSegment
:MotionSegment rdf:type owl:Class ;
               rdfs:subClassOf :MotionStep ,
                               [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty time:hasBeginning ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass time:Instant
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty time:hasEnd ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass time:Instant
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
               rdfs:comment """Constraints
(:hasShape exactly 1 geosparql:wktLiteral)
 and (:hasAvgSpeed exactly 1 rdfs:Literal)
 and (:hasMaxSpeed exactly 1 rdfs:Literal)
 and (:hasMinSpeed exactly 1 rdfs:Literal)"""^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#MotionStep
:MotionStep rdf:type owl:Class ;
            rdfs:subClassOf geosparql:Feature .


###  http://www.semanticweb.org/dmonto/autology/trip#Node
:Node rdf:type owl:Class ;
      rdfs:subClassOf geosparql:Feature ;
      rdfs:comment ""^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#Quantity
:Quantity rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#RegularCar
:RegularCar rdf:type owl:Class ;
            rdfs:subClassOf :Vehicle .


###  http://www.semanticweb.org/dmonto/autology/trip#Restriction
:Restriction rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#RoadFurniture
:RoadFurniture rdf:type owl:Class .


###  http://www.semanticweb.org/dmonto/autology/trip#RoadMatchedMotionStep
:RoadMatchedMotionStep rdf:type owl:Class ;
                       rdfs:subClassOf :MotionStep ;
                       rdfs:comment """Constraints
:onRoadSegment exactly 1 :RoadSegment"""^^xsd:string .


###  http://www.semanticweb.org/dmonto/autology/trip#RoadSegment
:RoadSegment rdf:type owl:Class ;
             rdfs:subClassOf geosparql:Feature ;
             rdfs:comment """A Route Link is a path segment (section of the route) in the routing network, such as a road.

Constraints
(:hasRestriction some :Restriction)
 and (:endsAtNode exactly 1 :Junction)
 and (:hasLinkLength exactly 1 :Distance)
 and (:startsAtNode exactly 1 :Junction)
 and (:hasSegmentID exactly 1 rdfs:Literal)
 and (:hasShape exactly 1 geosparql:wktLiteral)
 and (:hasFurniture some :RoadFurniture)
 and (:hasRoadName max 1 rdfs:Literal)"""@en .


###  http://www.semanticweb.org/dmonto/autology/trip#RoadSign
:RoadSign rdf:type owl:Class ;
          rdfs:subClassOf :RoadFurniture .


###  http://www.semanticweb.org/dmonto/autology/trip#Route
:Route rdf:type owl:Class ;
       rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasRouteLength ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass :Distance
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasFirstAbsLocation ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onDataRange geosparql:wktLiteral
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasFirstLocationName ;
                                                owl:cardinality "1"^^xsd:nonNegativeInteger
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasLastAbsLocation ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onDataRange geosparql:wktLiteral
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasLastLocationName ;
                                                owl:cardinality "1"^^xsd:nonNegativeInteger
